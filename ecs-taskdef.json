{
  "family": "swiggy-taskdef",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "1024",
  "memory": "2048",
  "executionRoleArn": "arn:aws:iam::475834860148:role/ecsTaskExecutionRole-swiggy",
  "containerDefinitions": [
    {
      "name": "swiggy-clone-1",
      "image": "475834860148.dkr.ecr.ap-south-1.amazonaws.com/swiggy-clone:latest",
      "portMappings": [
        {
          "containerPort": 5000,
          "protocol": "tcp"
        }
      ],
      "essential": true
    }
  ]
}
